      //$("#M3").removeClass("text-muted")
      //$("#M4").removeClass("text-muted")
      
      $("#e0").text(JSON.stringify(message, undefined, 4));
      
      
          })
      
      	
      socket.on('E1', function(message) {
            $("#e1").text(JSON.stringify(message, undefined, 4));
          })
       
       socket.on('E2', function(message) {
            $("#e2").text(JSON.stringify(message, undefined, 4));
          })
       
        socket.on('E3', function(message) {
      console.log(message)
            $("#e3").text(JSON.stringify(message, undefined, 4));
      info=message
      thead="<thead class='text-center'><tr><th class='text-center'>Details</th><th class='text-center'>Block number</th><th class='text-center'>Time stamp</th></tr></thead><tbody>"
      tbody =""
      console.log(info.length)
      for(var i=0; i<info.length; i++) 
      
      { tbody=tbody+"<tr class='spacer'> "+
      			"<td style='width:50%'>"+
      			'<div class="media-left media-middle">'+
      			'<a href="#" class="btn bg-primary-400 btn-rounded btn-icon btn-xs">'+
      			'<span class="letter-icon">S</span>'+
      			'</a>'+
      			'</div>'+
      			'<div class="media-body">'+
      			'<div class="media-heading">'+
      			'<a href="#" class="letter-icon-title">'+info[i].args.change+'</a>'+
      			'</div>'+
      			'<div class="text-muted text-size-small"><i class="icon-checkmark3 text-size-mini position-left"></i>'+info[i].event+'</div>'+
      			'</div>'+
      			"</td>"+
      			"<td>"+
      			'<span class="text-muted text-size-small">'+info[i].blockNumber+'</span>'+
      			"</td>"+
      			"<td>"+
      			'<span class="text-muted text-size-small">19/11/2016 14:00</span>'+
      			"</td>"+							
      			+"</tr>" }
      			
      tbody=thead+tbody+"</tbody>"
      $("#tab1").html(tbody)
      
      console.log(tbody)
      
          })
       
       
